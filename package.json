{
  "name": "omnichannel-backend",
  "version": "1.0.0",
  "description": "Backend omnicanal para sistema de mensajerÃ­a empresarial",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon server.js",
    "dev:frontend": "vite --config vite.config.ts",
    "prebuild": "rimraf dist",
    "build": "npm run build:frontend && npm run build:validate",
    "build:frontend": "vite build --config vite.config.ts",
    "build:validate": "node scripts/validateBuild.js",
    "postinstall": "npm run build",
    "preview": "npm run build && npm start",
    "clean": "rimraf dist .vite node_modules/.cache logs",
    "clean:all": "npm run clean && rimraf node_modules",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "node scripts/e2eTest.js",
    "lint": "eslint src/**/*.js client/**/*.{ts,tsx} --max-warnings 0",
    "lint:fix": "eslint src/**/*.js client/**/*.{ts,tsx} --fix",
    "security:audit": "npm audit --audit-level moderate",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "load-test": "artillery run tests/load/basic-load.yml",
    "stress-test": "artillery run tests/load/stress-test.yml",
    "create-admin": "node scripts/createAdmin.js",
    "backup": "node scripts/backup.js",
    "deploy:check": "npm run lint && npm run test:ci && npm run build",
    "health-check": "node scripts/healthCheck.js",
    "validate:env": "node scripts/validateEnv.js",
    "monitor": "clinic doctor -- node server.js",
    "flame": "clinic flame -- node server.js"
  },
  "keywords": [
    "omnichannel",
    "whatsapp",
    "twilio",
    "messaging",
    "crm",
    "firebase",
    "express"
  ],
  "author": "Tu Empresa",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "envalid": "^8.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "file-type": "^19.6.0",
    "firebase-admin": "^12.7.0",
    "glob": "^11.0.3",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.460.0",
    "mime-types": "^2.1.35",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "rate-limit-redis": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.28.1",
    "redis": "^4.7.0",
    "twilio": "^5.3.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "artillery": "^2.0.23",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "clinic": "^12.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.6.2",
    "jest-junit": "^16.0.0",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.32",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-empresa/omnichannel-backend.git"
  },
  "bugs": {
    "url": "https://github.com/tu-empresa/omnichannel-backend/issues"
  },
  "homepage": "https://github.com/tu-empresa/omnichannel-backend#readme"
}

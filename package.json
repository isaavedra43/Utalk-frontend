{
  "name": "package.json",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "tsc --noEmit && vite build --mode analyze",
    "build:prod": "tsc --noEmit && vite build --mode production",
    "postinstall": "echo 'Dependencias instaladas correctamente'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "preview": "vite preview --host 0.0.0.0 --port $PORT",
    "start": "npx serve -s dist -l $PORT",
    "clean:logs": "node scripts/clean-console-logs.js",
    "optimize-imports": "node scripts/optimize-imports.js",
    "analyze-bundle": "node scripts/bundle-analyzer.js",
    "analyze": "npm run build:analyze && npx vite-bundle-analyzer dist",
    "bundle-size": "npm run build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.85.0",
    "@types/jspdf": "1.3.3",
    "@types/react-window": "^1.8.8",
    "@types/xlsx": "0.0.35",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.12",
    "jspdf": "3.0.3",
    "jspdf-autotable": "5.0.2",
    "lucide-react": "0.541.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.8.0",
    "react-window": "^1.8.11",
    "recharts": "^3.1.2",
    "scheduler": "0.25.0",
    "serve": "^14.2.4",
    "socket.io-client": "^4.8.1",
    "xlsx": "0.18.5",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/scheduler": "0.26.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "5.0.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}

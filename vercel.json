{
  "framework": "sveltekit",
  "build": {
    "env": {
      "NODE_VERSION": "18.19.0"
    }
  },
  "functions": {
    "src/routes/login/+page.server.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
